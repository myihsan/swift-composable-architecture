ext":"Action"},{"kind":"text","text":">, "},{"text":"observe","kind":"externalParam"},{"kind":"text","text":" "},{"text":"toViewState","kind":"internalParam"},{"kind":"text","text":": "},{"kind":"keyword","text":"@escaping"},{"text":" (","kind":"text"},{"kind":"typeIdentifier","text":"State"},{"text":") -> ","kind":"text"},{"kind":"typeIdentifier","text":"ViewState"},{"kind":"text","text":", "},{"text":"send","kind":"externalParam"},{"kind":"text","text":" "},{"kind":"internalParam","text":"fromViewAction"},{"text":": ","kind":"text"},{"text":"@escaping","kind":"keyword"},{"kind":"text","text":" ("},{"text":"ViewAction","kind":"typeIdentifier"},{"text":") -> ","kind":"text"},{"kind":"typeIdentifier","text":"Action"},{"text":", ","kind":"text"},{"kind":"attribute","text":"@"},{"text":"ViewBuilder","kind":"attribute","preciseIdentifier":"s:7SwiftUI11ViewBuilderV"},{"kind":"text","text":" "},{"kind":"externalParam","text":"content"},{"kind":"text","text":": "},{"text":"@escaping","kind":"keyword"},{"kind":"text","text":" ("},{"text":"ViewStore","kind":"typeIdentifier","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/ViewStore","preciseIdentifier":"s:22ComposableArchitecture9ViewStoreC"},{"text":"<","kind":"text"},{"text":"ViewState","kind":"typeIdentifier"},{"kind":"text","text":", "},{"text":"ViewAction","kind":"typeIdentifier"},{"kind":"text","text":">) -> "},{"text":"Content","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"file","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"StaticString","preciseIdentifier":"s:s12StaticStringV","kind":"typeIdentifier"},{"kind":"text","text":" = "},{"text":"#fileID","kind":"keyword"},{"text":", ","kind":"text"},{"kind":"externalParam","text":"line"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:Su","text":"UInt"},{"text":" = ","kind":"text"},{"text":"#line","kind":"keyword"},{"kind":"text","text":")"}],"platforms":["macOS"]}]},{"parameters":[{"name":"store","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A store."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"A function that transforms store state into observable view state. All"},{"text":" ","type":"text"},{"text":"changes to the view state will cause the ","type":"text"},{"type":"codeVoice","code":"WithViewStore"},{"type":"text","text":" to re-compute its view."}],"type":"paragraph"}],"name":"toViewState"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A function that transforms view actions into store action."}]}],"name":"fromViewAction"},{"name":"content","content":[{"type":"paragraph","inlineContent":[{"text":"A function that can generate content from a view store.","type":"text"}]}]}],"kind":"parameters"},{"content":[{"type":"heading","text":"Discussion","anchor":"discussion","level":2},{"inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/WithViewStore"},{"type":"text","text":" will re-compute its body for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"any"}]},{"type":"text","text":" change to the state it holds. Often the"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Store"},{"text":" that we want to observe holds onto a lot more state than is necessary to render a","type":"text"},{"type":"text","text":" "},{"type":"text","text":"view. It may hold onto the state of child features, or internal state for its logic."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"It can be important to transform the "},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Store","isActive":true,"type":"reference"},{"type":"text","text":"â€™s state into something smaller for observation."},{"text":" ","type":"text"},{"type":"text","text":"This will help minimize the number of times your view re-computes its body, and can even avoid"},{"type":"text","text":" "},{"type":"text","text":"certain SwiftUI bugs that happen due to over-rendering."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The way to do this is to use the "},{"type":"codeVoice","code":"observe"},{"type":"text","text":" argument of this initializer. It allows you to"},{"text":" ","type":"text"},{"type":"text","text":"turn the full state into a smaller data type, and only changes to that data type will trigger"},{"text":" ","type":"text"},{"type":"text","text":"a body re-computation."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"For example, if your application uses a tab view, then the root state may hold the state"},{"type":"text","text":" "},{"type":"text","text":"for each tab as well as the currently selected tab:"}],"type":"paragraph"},{"code":["struct AppState {","  var activity: ActivityState","  var search: SearchState","  var profile: ProfileState","  var selectedTab: Tab","  enum Tab { case activity, search, profile }","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"In order to construct a tab view you need to observe this state because changes to"},{"type":"text","text":" "},{"type":"codeVoice","code":"selectedTab"},{"type":"text","text":" need to make SwiftUI update the visual state of the UI. However, you do not"},{"text":" ","type":"text"},{"type":"text","text":"need to observe changes to "},{"code":"activity","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"search"},{"type":"text","text":" and "},{"type":"codeVoice","code":"profile"},{"text":". Those are only necessary for","type":"text"},{"text":" ","type":"text"},{"text":"those child features, and changes to that state should not cause our tab view to re-compute","type":"text"},{"text":" ","type":"text"},{"text":"itself.","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["struct AppView: View {","  let store: Store<AppState, AppAction>","","  var body: some View {","    WithViewStore(self.store, observe: \\.selectedTab) { viewStore in","      TabView(selection: viewStore.binding(send: AppAction.tabSelected) {","        ActivityView(","          store: self.store.scope(state: \\.activity, action: AppAction.activity)","        )","        .tag(AppState.Tab.activity)","        SearchView(","          store: self.store.scope(state: \\.search, action: AppAction.search)","        )","        .tag(AppState.Tab.search)","        ProfileView(","          store: self.store.scope(state: \\.profile, action: AppAction.profile)","        )","        .tag(AppState.Tab.profile)","      }","    }","  }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"To read more about this performance technique, read the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Performance"},{"type":"text","text":" article."}]}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/SwiftUI","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/WithViewStore","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/WithViewStore\/init(_:observe:content:file:line:)"]]},"metadata":{"externalID":"s:22ComposableArchitecture13WithViewStoreVAASQRz7SwiftUI0D0R0_rlE_7observe4send7content4file4lineACyxq_q0_GAA0E0Cyqd__qd_0_G_xqd__cqd_0_q_cq0_AA0dE0Cyxq_Gcs12StaticStringVSutcr0_lufc","extendedModule":"ComposableArchitecture","modules":[{"name":"ComposableArchitecture"}],"roleHeading":"Initializer","symbolKind":"init","conformance":{"availabilityPrefix":[{"type":"text","text":"Available when"}],"conformancePrefix":[{"type":"text","text":"Conforms when"}],"constraints":[{"type":"codeVoice","code":"ViewState"},{"type":"text","