typeIdentifier"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"ID"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"Element"},{"text":"> ","kind":"text"},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"ID"},{"kind":"text","text":" : "},{"text":"Hashable","kind":"typeIdentifier","preciseIdentifier":"s:SH"},{"kind":"text","text":", "},{"text":"Element","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:22ComposableArchitecture15ReducerProtocolP","text":"ReducerProtocol","kind":"typeIdentifier","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/ReducerProtocol"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"parameters","parameters":[{"content":[{"inlineContent":[{"text":"A writable key path from parent state to an ","type":"text"},{"type":"codeVoice","code":"IdentifiedArray"},{"type":"text","text":" of child"},{"type":"text","text":" "},{"type":"text","text":"state."}],"type":"paragraph"}],"name":"toElementsState"},{"name":"toElementAction","content":[{"inlineContent":[{"type":"text","text":"A case path from parent action to child identifier and child actions."}],"type":"paragraph"}]},{"name":"element","content":[{"type":"paragraph","inlineContent":[{"text":"A reducer that will be invoked with child actions against elements of child","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"state."}]}]}]},{"kind":"content","content":[{"level":2,"type":"heading","text":"Return Value","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}]},{"content":[{"level":2,"type":"heading","anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"For example, if a parent feature holds onto an array of child states, then it can perform","type":"text"},{"type":"text","text":" "},{"text":"its core logic ","type":"text"},{"inlineContent":[{"text":"and","type":"text"}],"type":"emphasis"},{"text":" the child’s logic by using the ","type":"text"},{"code":"forEach","type":"codeVoice"},{"text":" operator:","type":"text"}]},{"code":["struct Parent: ReducerProtocol {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case row(id: Row.State.ID, action: Row.Action)","    \/\/ ...","  }","","  var body: some ReducerProtocol<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \/Action.row) {","      Row()","    }","  }","}"],"type":"codeListing","syntax":"swift"},{"content":[{"inlineContent":[{"type":"text","text":"We are using "},{"code":"IdentifiedArray","type":"codeVoice"},{"text":" from our","type":"text"},{"text":" ","type":"text"},{"identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections","type":"reference","isActive":true},{"type":"text","text":" library because it provides a safe"},{"type":"text","text":" "},{"text":"and ergonomic API for accessing elements from a stable ID rather than positional indices.","type":"text"}],"type":"paragraph"}],"type":"aside","name":"Tip","style":"tip"},{"inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" forces a specific order of operations for the child and parent features. It"},{"type":"text","text":" "},{"text":"runs the child first, and then the parent. If the order was reversed, then it would be","type":"text"},{"type":"text","text":" "},{"text":"possible for the parent feature to remove the child state from the array, in which case the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"child feature would not be able to react to that action. That can cause subtle bugs."}],"type":"paragraph"},{"inlineContent":[{"text":"It is still possible for a parent feature higher up in the application to remove the child","type":"text"},{"type":"text","text":" "},{"type":"text","text":"state from the array before the child has a chance to react to the action. In such cases a"},{"type":"text","text":" "},{"text":"runtime warning is shown in Xcode to let you know that there’s a potential problem.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"references":{"doc://ComposableArchitecture/documentation/ComposableArchitecture/EmptyReducer":{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/EmptyReducer","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"EmptyReducer"}],"type":"topic","title":"EmptyReducer","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"EmptyReducer"}],"url":"\/documentation\/composablearchitecture\/emptyreducer","abstract":[{"type":"text","text":"A reducer that does nothing."}],"role":"symbol"},"doc://ComposableArchitecture/documentation/ComposableArchitecture/EmptyReducer/forEach(_:action:_:file:fileID:line:)":{"type":"topic","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/EmptyReducer\/forEach(_:action:_:file:fileID:line:)","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"forEach","kind":"identifier"},{"text":"<","kind":"text"},{"text":"ID","kind":"genericParameter"},{"kind":"text","text":", "},{"kind":"genericParameter","text":"Element"},{"kind":"text","text":">("},{"kind":"typeIdentifier","text":"WritableKeyPath","preciseIdentifier":"s:s15WritableKeyPathC"},{"text":"<","kind":"text"},{"text":"Self","kind":"typeIdentifier"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"State","preciseIdentifier":"s:22ComposableArchitecture15ReducerProtocolP5StateQa"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"IdentifiedArray","preciseIdentifier":"s:21IdentifiedCollections0A5ArrayV"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"ID"},{"kind":"text","text":"