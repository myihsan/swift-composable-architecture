ion {","    case child(Child.Action)","    \/\/ ...","  }","","  var body: some ReducerProtocol<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .ifLet(\\.child, action: \/Action.child) {","      Child()","    }","  }","}"]},{"inlineContent":[{"text":"The ","type":"text"},{"code":"ifLet","type":"codeVoice"},{"text":" forces a specific order of operations for the child and parent features. It runs","type":"text"},{"type":"text","text":" "},{"text":"the child first, and then the parent. If the order was reversed, then it would be possible for","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the parent feature to "},{"type":"codeVoice","code":"nil"},{"text":" out the child state, in which case the child feature would not be","type":"text"},{"text":" ","type":"text"},{"text":"able to react to that action. That can cause subtle bugs.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is still possible for a parent feature higher up in the application to "},{"type":"codeVoice","code":"nil"},{"text":" out child","type":"text"},{"type":"text","text":" "},{"text":"state before the child has a chance to react to the action. In such cases a runtime warning","type":"text"},{"type":"text","text":" "},{"type":"text","text":"is shown in Xcode to let you know that there’s a potential problem."}]}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/composablearchitecture\/scope\/iflet(_:action:then:file:fileid:line:)"]}],"sections":[],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Scope\/ifLet(_:action:then:file:fileID:line:)"},"metadata":{"symbolKind":"method","title":"ifLet(_:action:then:file:fileID:line:)","roleHeading":"Instance Method","role":"symbol","modules":[{"name":"ComposableArchitecture"}],"externalID":"s:22ComposableArchitecture1